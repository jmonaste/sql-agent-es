# LLM Provider Configuration
LLM_PROVIDER=openai
# Options: 'openai' or 'docker_runner'

# OpenAI Configuration (required if LLM_PROVIDER=openai)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4

# Docker Model Runner Configuration (used if LLM_PROVIDER=docker_runner)
DOCKER_RUNNER_MODEL=ai/smollm2:latest

